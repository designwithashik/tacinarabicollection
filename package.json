{
  "name": "tacin-arabi-collection",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:pages": "npx @cloudflare/next-on-pages",
    "preview:pages": "wrangler pages dev .vercel/output/static",
    "deploy:worker": "wrangler deploy",
    "deploy:worker:dry": "wrangler deploy --dry-run",
    "deploy:pages": "wrangler pages deploy .vercel/output/static --project-name=tacin-arabi",
    "postinstall": "npm --prefix worker install --no-fund --no-audit"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "^14.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^11.3.19",
    "lucide-react": "^0.469.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.2.73",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  }
}
